{
    "plugins": [
        "github",
        "anchor-navigation-ex"
    ],
    "pluginsConfig": {
        "github": {
            "url": "https://github.com/hjcian/inertia-app"
        },
        "anchor-navigation-ex": {
            "associatedWithSummary":false,
            "showLevel":false,
            "multipleH1": false,
            "mode": "float",
            
            "pageTop": {
                "showLevelIcon": false,
                "level1Icon": "fa fa-hand-o-right",
                "level2Icon": "fa fa-hand-o-right",
                "level3Icon": "fa fa-hand-o-right"
                }
        },
        "fontSettings": {
            "theme": "night",
            "family": "sans",
            "size": 1
        }
    }
}